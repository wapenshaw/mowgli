			"settings": {
				"foreground": "#57a669"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#A6B2C0"
			}
		},
		{
			"name": "Function Parameters",
			"scope": "meta.function.parameters",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#818992",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comment Markup Link",
			"scope": "comment markup.link",
			"settings": {
				"foreground": "#496C99"
			}
		},
		{
			"name": "markup diff",
			"scope": "markup.changed.diff",
			"settings": {
				"foreground": "#e0c28550"
			}
		},
		{
			"name": "diff",
			"scope": "meta.diff.header.from-file,punctuation.definition.from-file.diff",
			"settings": {
				"foreground": "#569cd650"
			}
		},
		{
			"name": "inserted.diff",
